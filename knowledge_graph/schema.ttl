@prefix aql: <https://aql.webis.de/> .
@prefix schema: <http://schema.org/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix http: <https://www.w3.org/2011/http#> .

# Note: AQL internal properties are not modeled.

#################################################################
# AQL Extension Classes
#################################################################

aql:SERP a rdfs:Class ;
  rdfs:subClassOf schema:SearchResultsPage , dct:Collection ;
  rdfs:label "SERP" .

aql:ResultBlock a rdfs:Class ;
  rdfs:subClassOf schema:CreativeWork , schema:ListItem ;
  rdfs:label "Result Block" ;
  rdfs:comment "Organic result block or feature block shown on SERP." .

aql:WebSearchResultBlock a rdfs:Class ;
  rdfs:subClassOf aql:ResultBlock ;
  rdfs:label "Web Search Result Block" ;
  rdfs:comment "A web search result block on a SERP." .

aql:SpecialContentResultBlock a rdfs:Class ;
  rdfs:subClassOf aql:ResultBlock ;
  rdfs:label "Special Content Result Block" ;
  rdfs:comment "A direct answer/ featured result block on a SERP." .

aql:Result a rdfs:Class ;
  rdfs:subClassOf schema:WebPage ;
  rdfs:label "Result" ;
  rdfs:comment "Landing Page" .

aql:Capture a rdfs:Class ;
  rdfs:subClassOf schema:WebPage ;
  rdfs:label "Capture" .

aql:Provider a rdfs:Class ;
  rdfs:subClassOf schema:Service ;
  rdfs:label "Provider" .

aql:Archive a rdfs:Class ;
  rdfs:subClassOf schema:ArchiveOrganization ;
  rdfs:label "Archive" .

#################################################################
# AQL Extension Properties
#################################################################

# SERP properties
aql:trecTaskURL a rdf:Property ;
  rdfs:domain aql:SERP ;
  rdfs:range xsd:anyURI ;
  rdfs:label "TREC Task URL" ;
  rdfs:comment "The URL of the TREC task associated with the SERP." .

# ResultBlock properties
aql:xPath a rdf:Property ;
  rdfs:domain aql:ResultBlock ;
  rdfs:range xsd:string ;
  rdfs:label "XPath" ;
  rdfs:comment "The XPath expression to locate the result block on the SERP." .

aql:xmlContents a rdf:Property ;
  rdfs:domain aql:ResultBlock ;
  rdfs:range xsd:string ;
  rdfs:label "XML Contents" ;
  rdfs:comment "The XML contents of the result block." .

# WebSearchResultBlock properties
aql:rank a rdf:Property ;
  rdfs:domain aql:WebSearchResultBlock ;
  rdfs:range xsd:integer ;
  rdfs:label "Rank" ;
  rdfs:comment "The rank of the web search result block on the SERP." .

# Capture properties
aql:digest a rdf:Property ;
  rdfs:domain aql:Capture ;
  rdfs:range xsd:string ;
  rdfs:label "Digest" ;
  rdfs:comment "A digest of the capture, e.g., a hash or checksum." . #TODO this correct?

aql:mementoAPIViewerURL a rdf:Property ;
  rdfs:domain aql:Capture ;
  rdfs:subPropertyOf schema:archivedAt ;
  rdfs:range xsd:anyURI ;
  rdfs:label "Memento API Viewer URL" .
  # rdfs:comment "URL to view the capture using the Memento API." . #TODO

aql:mementoAPIRawURL a rdf:Property ;
  rdfs:domain aql:Capture ;
  rdfs:subPropertyOf schema:archivedAt ;
  rdfs:range xsd:anyURI ;
  rdfs:label "Memento API Raw contents URL" .
  # rdfs:comment "URL to access the raw capture using the Memento API." . #TODO

# Provider properties
aql:domain a rdf:Property ;
  rdfs:domain aql:Provider ;
  rdfs:range xsd:string ;
  rdfs:label "Domain" ;
  rdfs:comment "The hostname/domain of a search result or provider." .

aql:hostname a rdf:Property ;
  owl:equivalentProperty aql:domain ;
  rdfs:label "Hostname" .

aql:urlPathPrefix a rdf:Property ;
  rdfs:domain aql:Provider ;
  rdfs:range xsd:string ;
  rdfs:label "URL Path Prefix" ;
  rdfs:comment "The URL path prefix of a search result or provider." .

aql:wikiDataURL a rdf:Property ;
  rdfs:domain aql:Provider ;
  rdfs:subPropertyOf schema:url ;
  rdfs:range xsd:anyURI ;
  rdfs:label "WikiData URL" ;
  rdfs:comment "URL to the WikiData entry of the provider." .

# Archive properties
aql:mementoAPIBaseURL a rdf:Property ;
  rdfs:domain aql:Archive ;
  rdfs:subPropertyOf schema:url ;
  rdfs:range xsd:anyURI ;
  rdfs:label "Memento API Base URL" ;
  rdfs:comment "Base URL for the Memento API of the archive." .

aql:cdxAPIBaseURL a rdf:Property ;
  rdfs:domain aql:Archive ;
  rdfs:subPropertyOf schema:url ;
  rdfs:range xsd:anyURI ;
  rdfs:label "CDX API Base URL" ;
  rdfs:comment "Base URL for the CDX API of the archive." .

aql:wikiDataURL a rdf:Property ;
  rdfs:domain aql:Archive ;
  rdfs:subPropertyOf schema:url ;
  rdfs:range xsd:anyURI ;
  rdfs:label "WikiData URL" ;
  rdfs:comment "URL to the WikiData entry of the archive." .

#################################################################
# Properties from other vocabularies
#################################################################

# SERP properties
schema:identifier
  rdfs:domain aql:SERP ;
  rdfs:range xsd:string .

schema:identifier a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:identifier . #TODO do we need the labels here?

schema:name
  rdfs:domain aql:SERP ;
  rdfs:range xsd:string .

schema:name a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty skos:prefLabel .

# WebSearchResultBlock properties
schema:title
  rdfs:domain aql:WebSearchResultBlock ;
  rdfs:label "Snippet Title" ;
  rdfs:range xsd:string .

schema:title a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:name .

schema:abstract
  rdfs:domain aql:WebSearchResultBlock ;
  rdfs:label "Snippet Text" ;
  rdfs:range xsd:string .

schema:abstract a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:abstract .

# Capture properties
schema:dateCreated
  rdfs:domain aql:Capture ;
  rdfs:range xsd:dateTime .

schema:dateCreated a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:created .

schema:url
  rdfs:domain aql:Capture ;
  rdfs:label "Original URL" ;
  rdfs:range xsd:anyURI .

schema:url a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty http:absoluteURI ;
  rdfs:label "url" .

http:statusCodeNumber
  rdfs:domain aql:Capture ;
  rdfs:range xsd:integer ;
  rdfs:label "HTTP Status Code" ;
  rdfs:comment "The HTTP status code of the capture." . #TODO make the comments and labels uniform

http:statusCodeNumber a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty aql:httpStatusCode ; # TODO this aql resources is not defined beforehand
  rdfs:label "Status Code Number" .

schema:encodingFormat
  rdfs:domain aql:Capture ;
  rdfs:range xsd:string ;
  rdfs:label "MIME Type" ;
  rdfs:comment "The MIME Type of the capture, e.g., 'text/html'." .

# TODO internet archive metadata / collection

# Provider properties
schema:identifier
  rdfs:domain aql:Provider ;
  rdfs:range xsd:string .

schema:identifier a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:identifier .

schema:name
  rdfs:domain aql:Provider ;
  rdfs:range xsd:string .

schema:name a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty skos:prefLabel .

# Archive properties
schema:identifier
  rdfs:domain aql:Archive ;
  rdfs:range xsd:string . 

schema:identifier a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:identifier .

schema:name
  rdfs:domain aql:Archive ;
  rdfs:range xsd:string .   

schema:name a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty skos:prefLabel .

# Result properties
schema:identifier
  rdfs:domain aql:Result ;
  rdfs:range xsd:string .

schema:identifier a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:identifier .

schema:url
  rdfs:domain aql:Result ;
  rdfs:label "Original URL" ;
  rdfs:range xsd:anyURI .

schema:url a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty http:absoluteURI.

schema:title
  rdfs:domain aql:Result ;
  rdfs:label "Page Title" ;
  rdfs:range xsd:string .

schema:title a rdf:Property ; # TODO is that a good way to describe equivalent property?
  owl:equivalentProperty dct:title .